    <section id="main-content">
      <section class="wrapper">
        <div class="row">

          <div class="col-lg-12">
            <h3 class="page-header"><i class="fa fa-file-text-o"></i> Form </h3>
            <ol class="breadcrumb">
              <li><i class="fa fa-home"></i><a href="/adminhome">Home</a></li>
              <li><i class="icon_document_alt"></i>Forms</li>
              <li><i class="fa fa-file-text-o"></i>Form elements</li>  
            </ol>


          </div>
        </div>
        <br>
        <!-- d-none -->
        <div class="row">
          <div class="col-lg-12">
            <section class="panel">
              <header class="panel-heading">
                Form Elements
              </header>


              <div class="container mt-3">
                <form action="#" method="post" class="form-horizontal ">
                      
                  <div class="custom-file col-lg-12 mb-3">
                    <input type="file" class="custom-file-input" id="customFile" name="filename">
                    <label class="custom-file-label" for="customFile">Choose csv File</label>
                  </div>

                  <div class="mt-3" form-group>
                    <button type="button" class="form-control btn btn-primary" onclick="printData()">Submit</button>
                  </div>
                </form>
              </div>

                <hr>
                <div id="values">
                  
                </div>
              <div class="panel-body">

                <form class="form-horizontal" id="form" method="post" action="/success">

                  <div class="form-group">
                    <label class="col-sm-2 control-label">First Name</label>
                    <div class="col-sm-10">
                      <input type="text" class="form-control" name="fname" required>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="col-sm-2 control-label">Last Name</label>
                    <div class="col-sm-10">
                      <input type="text" class="form-control" name="lname">
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="control-label col-lg-2" for="select" required>Department</label>
                    <div class="col-lg-10">
                      <select class="form-control" name="department">
                        <option value="" selected disabled></option>
                            <option>ECE</option>
                            <option>CSE</option>
                      </select>
                    </div>
                  </div>
                  

                  <div class="form-group">
                    <label class="col-sm-2 control-label">Type</label>
                    <div class="col-sm-10">
                      <input type="text" class="form-control" name="type" required>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="control-label col-lg-2" for="select" required>Institute Name</label>
                    <div class="col-lg-10">
                      <select class="form-control" name="institute">
                        <option value="" selected disabled></option>
                            <option>Indian Institute of Information Technology Guwahati</option>
                            <option>Indian Institute of Technology Guwahati</option>
                            <option>Guwahati University</option>
                      </select>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="col-sm-2 control-label">Registration Id</label>
                    <div class="col-sm-10">
                      <input type="text" class="form-control" name="reg_id" required>
                    </div>
                  </div>

                  <div class="form-group">
                      <label class="col-sm-2 control-label" for="join_date">Joining Date</label>
                      <div class="col-sm-10">
                        <input type="date" id="join_date" class="form-control" name="join_date" required>
                      </div>  
                  </div>

                  <div class="form-group">
                    <label class="col-sm-2 control-label">Email</label>
                    <div class="col-sm-10">
                      <input type="text" class="form-control" name="email" required>
                    </div>
                  </div>

                  <div class="form-group">
                    <label class="col-sm-2 control-label">Password</label>
                    <div class="col-sm-10">
                      <input class="form-control" type="password" name="pass" id="pass" required>
                    </div>
                  </div>

                  <button class = "generate" id="gen_pass" type="button">
                    Generate Password
                  </button>

                  <input type="submit" name="Register" class="form-control">
                </form>

              </div>

            </section>

          </div>
        </div>
        <!-- Basic Forms & Horizontal Forms-->
        <!-- page end-->
      </section>
